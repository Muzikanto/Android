<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent" android:layout_height="match_parent"
             android:theme="@style/Blanks" tools:context="com.example.test.Fragment.Blank3"
             android:id="@+id/FragBl3_mainLay">


    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <ImageView
                android:src="@drawable/ic_launcher_background"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/FragBl3_ImageView"
                android:layout_marginTop="40dp"
                android:layout_alignParentTop="true" android:layout_centerHorizontal="true"/>
        <Button
                android:text="Load"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/FragBl3_LoadImages"
                android:layout_below="@+id/FragBl3_ImageView"
                android:layout_centerHorizontal="true" android:layout_marginTop="28dp"/>
    </RelativeLayout>
</FrameLayout>
